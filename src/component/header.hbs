{{#with navigation}}
<nav>
    <div class="card">
        <div class="row">
            <div class="col-12">
                <div class="nav nav-tabs">
                    {{#each links}}
                    <div class="nav-item ">
                        <a href="{{href}}" class="nav-link">{{text}}</a>
                    </div>
                    {{/each}}
                    <a href="{{href}}" class="nav-link disabled">Disabled</a>
                </div>
            </div>
        </div>
    </div>
</nav>
{{/with}}